<div class='container'>
  <div class='etching col-md-3'>
    <h2><%= @etching.title %></h2>
    <p>Summary: <%= @etching.short_description %></p>
    <p>Description: <%= @etching.long_description %></p>
  </div>

  <% @etching.prints.each do |print| %>
    <img src="<%= print.thumbnail_url %>" />
    <p><%= print.notes %></p>
  <% end %>
</div>
