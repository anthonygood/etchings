<section class='container'>
  <h1>Etchings#index</h1>
  <p>Find me in app/views/etchings/index.html.erb</p>

  <% @etchings.each do |etching| %>
    <%= link_to etching do %>
      <div class='etching col-md-3'>
        <h2><%= etching.title %></h2>
        <p>Short: <%= etching.short_description %></p>
        <p>Year: <%= etching.year %></p>
        <p>Long: <%= etching.long_description %></p>
        <p>Notes: <%= etching.notes %></p>
        <p>Height: <%= etching.height %>mm</p>
        <p>Width: <%= etching.width %>mm</p>
        <p>Plates: <%= etching.plates %></p>
        <p>Print run: <%= etching.print_run %></p>
        <p>Listed: <%= etching.listed %></p>
      </div>
    <% end %>
  <% end %>

</section>
