<section class='container'>
  <h1>Etchings#index</h1>
  <p>Find me in app/views/etchings/index.html.erb</p>

  <% @etchings.each do |etching| %>
    <%= link_to etching do %>
      <div class='etching col-md-3'>
        <h2><%= etching.title %></h2>
        <p><%= etching.long_description %></p>
        <p><%= etching.short_description %></p>
        <p><%= etching.height %></p>
        <p><%= etching.width %></p>
        <p><%= etching.plates %></p>
      </div>
    <% end %>
  <% end %>

</section>
